{
    "name": "audio-processing-backend",
    "version": "1.0.0",
    "description": "Backend asíncrono para procesamiento de audio de larga duración",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "npx prisma generate && npx prisma migrate deploy && node dist/index.js",
        "start:worker": "npx prisma generate && node dist/workers/audioWorker.js",
        "dev": "ts-node-dev src/index.ts",
        "worker": "ts-node-dev src/workers/audioWorker.ts"
    },
    "dependencies": {
        "@google/generative-ai": "^0.24.1",
        "@supabase/supabase-js": "^2.39.0",
        "@types/multer": "^2.0.0",
        "@types/uuid": "^10.0.0",
        "bullmq": "^5.1.0",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
